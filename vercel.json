{
  "version": 2,
  "installCommand": "npm install && cd admin-dashboard && npm install",
  "buildCommand": "cd admin-dashboard && npm run build",
  "functions": {
    "server.js": {
      "includeFiles": "admin-dashboard/dist/**"
    }
  },
  "rewrites": [
    { "source": "/api/(.*)", "destination": "/server.js" },
    { "source": "/admin(.*)", "destination": "/server.js" }
  ]
}
